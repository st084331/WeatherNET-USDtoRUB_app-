<Page x:Class="WeatherNETBrows.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WeatherNETBrows"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1" Loaded="Page_Loaded" Background="White">
    <Grid>
        <Label x:Name="Exchangerate" Content="Exchangerate: No Info" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Currencyfreaks" Content="Currencyfreaks: No Info" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Currencyapi" Content="Currencyapi: No Info" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Openexchangerates" Content="Openexchangerates: No Info" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top"/>
        <Button x:Name="UpdateButton" Content="Update" Margin="10,134,0,0" VerticalAlignment="Top" Height="25" HorizontalAlignment="Left" Width="141" Click="UpdateButton_Click"/>
        <WpfPlot x:Name="RegressionPlot"  HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Height="334" Width="502" IsManipulationEnabled="False"/>
    </Grid>
</Page>
